take(){
    mkdir $1; cd $1
}

mountie(){
    python3 /home/maotora/mountie/mountie.py
}
retake(){
    current=$(basename `pwd`)
    mv --verbose * ../; cd ..; rmdir $current
}

multipleRedirect(){
    fileOne=$1
    fileTwo=$2
    command=$3
    param=$4

    $command $param >> $fileOne; $command $param >> $fileTwo

}

utube(){
    url=$1
    youtube-dl --continue --max-quality best --format best "$url"
}
